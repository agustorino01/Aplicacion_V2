<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Main2Activity_buscar">

    <SearchView
            android:layout_width="367dp"
            android:layout_height="56dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="22dp" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="16dp" android:id="@+id/searchView"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="@string/nombre"
            android:ems="10"
            android:id="@+id/buscar_n"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="32dp"
            android:autofillHints="" android:layout_marginTop="68dp"
            app:layout_constraintTop_toBottomOf="@+id/searchView"
            android:hint="@string/nombre1"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="@string/dni"
            android:ems="10"
            android:id="@+id/buscar_d"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="22dp"
            android:layout_marginBottom="82dp"
            app:layout_constraintBottom_toTopOf="@+id/buscar_s" android:autofillHints=""
            android:hint="@string/dni"/>
    <EditText
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:inputType="textPersonName"
            android:text="@string/sector"
            android:ems="10"
            android:id="@+id/buscar_s"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="21dp"
            android:layout_marginBottom="183dp"
            app:layout_constraintBottom_toBottomOf="parent" android:autofillHints=""
            android:hint="@string/sector"/>
</android.support.constraint.ConstraintLayout>